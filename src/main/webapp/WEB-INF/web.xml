	</servlet>
	<servlet-mapping>
		<servlet-name>ItemsBoughtServlet</servlet-name>
		<url-pattern>/ItemsBoughtServlet</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>AddReviewServlet</servlet-name>
